@model Academic_Cert
    <div class="row" dir="rtl">
        <div class="col-md-8 offset-2 ">
            <p>
                <h1 class="float-right">شهادة الحاصل عليها / اضافة جديد</h1>
            </p>
            <br />
            <br />
            @*<br />
                <br />*@
            <div class="row">
                <div class="col-md-10 offset-2">
                    <form enctype="multipart/form-data" asp-action="Create">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label asp-for="Academic_Degree" class="control-label float-right">التحصيل العلمي</label>
                                    <input asp-for="Academic_Degree" class="form-control" />
                                    <span asp-validation-for="Academic_Degree" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="University" class="control-label float-right">الجامعة</label>
                                    <input asp-for="University" class="form-control" />
                                    <span asp-validation-for="University" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="College" class="control-label float-right">الكلية</label>
                                    <input asp-for="College" class="form-control" />
                                    <span asp-validation-for="College" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Department" class="control-label float-right">القسم</label>
                                    <input asp-for="Department" class="form-control" />
                                    <span asp-validation-for="Department" class="text-danger"></span>
                                </div>             
                            </div>
                            <div class="col-md-6">
                                 <div class="form-group">
                                    <label asp-for="Specilisit" class="control-label float-right">الاختصاص</label>
                                    <input asp-for="Specilisit" class="form-control" />
                                    <span asp-validation-for="Specilisit" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Graduate_Year" class="control-label float-right">سنة التخرج</label>
                                    <input asp-for="Graduate_Year" class="form-control" />
                                    <span asp-validation-for="Graduate_Year" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Country" class="control-label float-right">البلد المانح للشهادة</label>
                                    <input asp-for="Country" class="form-control" />
                                    <span asp-validation-for="Country" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Personal" class="control-label float-right">الاسم</label>
                                    <input asp-for="Personal" class="form-control"/>
                                    <span asp-validation-for="Personal" class="text-danger"></span>
                                </div>                               
                            </div>
                        </div>
                        <div class="form-group">
                            <label asp-for="Notes" class="control-label float-right">الملاحظات</label>
                            <textarea asp-for="Notes" class="form-control" ></textarea>
                            <span asp-validation-for="Notes" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="pdf" class="control-label float-right">ارفاق ملف</label>
                            <div class="custom-file">
                                <input asp-for="pdf" class="custom-file-input" id="customFile">
                                <label class="custom-file-label" for="customFile">Choose file</label>
                            </div>
                            <span asp-validation-for="pdf" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Create" class="btn btn-outline-success float-right" />
                            <a class="btn btn-outline-secondary" asp-action="Index">Show All</a>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>




